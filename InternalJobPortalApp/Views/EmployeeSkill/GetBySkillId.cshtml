        <th>
            @Html.DisplayNameFor(model => model.SkillId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SkillExp)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EmpId)
        </td>
